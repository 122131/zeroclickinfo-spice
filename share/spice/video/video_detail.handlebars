{{! Put the iframe here}}<iframe width="100%" height="340" src="https://www.youtube-nocookie.com/embed/{{id}}" frameborder="0" allowfullscreen></iframe><div>  <a href='http://www.youtube.com/watch?v={{id}}' title="{{title}}"><b>{{title}}</b></a></div><div class="description">{{description}}</div>{{! Only show this part if the video is in the category "Music"}}{{#checkMusic category title}}  <span>Search on:</span>  <img src="https://icons.duckduckgo.com/i/www.apple.com.ico" class="favicon">  <a href='javascript:void(0);' onclick='ddg_spice_video.set_itunes(this);' data-title='{{title}}' data-artist='{{artist}}' data-song='{{song}}' id='itunes'>iTunes</a>    <img src="https://icons.duckduckgo.com/i/www.spotify.com.ico" class="favicon">  <a href='/?q=!spotify {{title}}'>Spotify</a>  <img src="https://icons.duckduckgo.com/i/www.amazon.com.ico" class="favicon">  <a href='/?q=!a {{title}}'>Amazon</a>{{/checkMusic}}