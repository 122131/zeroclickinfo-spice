{{! Put the iframe here}}<iframe width="100%" height="340" src="https://www.youtube-nocookie.com/embed/{{id}}" frameborder="0" allowfullscreen></iframe><div>  <a href='http://www.youtube.com/watch?v={{id}}' title="{{title}}"><b>{{title}}</b></a></div><div class="description">{{description}}</div>{{! Only show this part if the video is in the category "Music"}}{{#checkMusic category title}}<a id="expand" href="javascript:;">  <span>Search on...</span></a><div id="all" style="display: none;">  <table>    <tbody>      <tr>        <td class="more-links">          <a href='javascript:void(0);' onclick='ddg_spice_video.set_itunes(this);' data-title='{{title}}' data-artist='{{artist}}' data-song='{{song}}' id='itunes'>iTunes</a>        </td>        <td class="more-links">          <a href='/?q=!spotify {{title}}'>Spotify</a>        </td>      </tr>      <tr>        <td class="more-links">          <a href='/?q=!rdio {{title}}'>Rdio</a>        </td>        <td class="more-links">          <a href='/?q=!a {{title}}'>Amazon</a>        </td>      </tr>    </tbody>  </table></div>{{/checkMusic}}